{
    "parser": "babel-eslint",
    "plugins": [
        "svelte3"
    ],
    "rules": {
        "react/prop-types": [ 0 ]
    },
    "overrides": [
        {
            "files": "*.svelte",
            "processor": "svelte3/svelte3"
        }
    ],
    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "__DEVELOPMENT__": false
    }
}
